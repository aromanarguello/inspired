<div class="main-container">
  <div class="right-container">
    <hr class="top-hr" />
    <div class="title-text-container">
      <h2>
        <div class="science-letter">
          Science
        </div>
        <div class="science-letter light-color-letters">
          & the Big Question
        </div>
      </h2>
    </div>
    <form class="search-form">
      <input id="search-bar" type="text" name="" placeholder="Search">
      <i class="ion-search search-icon"></i>
    </form>
    <div class="data-list-container data-list-left">
      <% seed.forEach( oneSeed => { %>
        <%if (oneSeed.company.length < 18) {%>
        <ul>
          <li>
            <a href="/"><%=oneSeed.company%></a>
          </li>
        </ul>
        <%}%>
      <%});%>
    </div>
    <div class="data-list-parent">
    <div class="data-list-container data-list-right">
      <% seedSorted.forEach( oneSeed => { %>
        <%if (oneSeed.company.length > 20) {%>
        <ul>
          <li>
            <a href="/"><%=oneSeed.company%></a>
          </li>
        </ul>
      <%}%>
      <%});%>
    </div>
    </div>
  </div> <!-- Right container -->

  <div class="left-container">
    <hr class="top-hr" />
    <ul class="nav-links">
      <li class="home-link"><a href="/"> Home </a></li>
      <li> About </li>
      <li> Contact </li>
      <li> A-Z Index </li>
    </ul>
    <div id='form-parent'>
      <div class="left-form-container">
      <div class="left-form-float">
        <form id="schedule-input" method="post" action="/">
          <label class="label-text" for="firstNameInput">First Name:</label>
          <br />
          <input id="firstNameInput" type="text" name="firstName" placeholder="e.g. John">
          <br />
          <label class="label-text" for="lastNameInput">Last Name:</label>
          <br />
          <input id="lastNameInput" type="text" name="lastName" placeholder="e.g. Smith">
          <br />
          <label class="label-text" for="emailInput">Email:</label>
          <br />
          <input id="emailInput" type="email" name="email" placeholder="e.g. johnsmith@gmail.com">
          <br />
          <label class="label-text message-label" for="messageInput">Write your message:</label>
          <br />
          <textarea maxLength="140" id="messageInput" type="text" name="message" placeholder="Please write a short message here..."></textarea>
          <br />
          <button class="submit-btn">Submit</button>
        </form>
      </div>
    </div>
        <div class="blockquote-container">
          <blockquote>
          <em>"Always deliver more than expected"</em>
          <p><em>-Larry Page</em></p>
        </div> <!-- left-form-container -->
        </blockquote>
        </div>
    <br />
    <hr class="top-hr" />
    <div class="post-container">
      <div class="header-container">
      </div><!-- post-container -->
        <div class="content-container">
        </div> <!-- content-container -->
  </div> <!-- post-container -->
</div> <!-- Left container -->
